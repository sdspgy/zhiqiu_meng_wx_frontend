<!--pages/welcome/welcome.wxml-->
<van-cell custom-class title="地址" title-width="" icon="location-o" is-link="true" bind:click="addEvent" />
<van-cell custom-class title="小程序" title-width="" icon="shop-o" is-link="true" bind:click="programEvent" />

<!--绑定点击事件-->
<button bindtap="nearby_search">{{add}}</button>
<!--地图容器-->
<map id="myMap" markers="{{markers}}" style="width:100%;height:300px;" longitude="107.265053" latitude="29.750486" scale='16'>
</map>
<!--绑定输入事件-->
<input style="border:1px solid black;" bindinput="getsuggest" value="{{backfill}}"></input>
<!--关键词输入提示列表渲染-->
<view wx:for="{{suggestion}}" wx:key="index">
    <!--绑定回填事件-->
    <view>
    <!--根据需求渲染相应数据-->
    <!--渲染地址title-->
    <view style="text-align:center;" bindtap="backfill" id="{{index}}">{{item.title}}</view>
    <!--渲染详细地址-->
    <view style="font-size:12px;color:#666;text-align:center;">{{item.addr}}</view>
    </view>
</view>